<% if (this.model) { %>
  <a href='#' class='done_editing'>Done</a>
  <div id='errors'></div>
  <div>
    <label for="resume_name">Resume name: </label>
    <input type="text" id="resume_name" value="<%= this.model.get('name') %>">
  <div id='profile'>
    <h3>Profile</h3>
    <a href='#' class='new_profile'>New Profile</a>
  </div>
  <div id='address'>
    <h3>Address</h3>
    <a href='#' class='new_address'>New Address</a>
  </div>
<% } else { %>
  <p>No Resume</p>
<% } %>
