<h3>Edit Item</h3>
<div>
  <label for="new-item-name">Name:</label>
  <input type="text" id="new-item-name" value="<%= this.model.get('name') %>">
</div>
<div>
  <label for="new-item-title">Title:</label>
  <input type="text" id="new-item-title" value="<%= this.model.get('title') %>">
</div>
<div>
  <label for="new-item-heading">Heading:</label>
  <input type="text" id="new-item-heading" value="<%= this.model.get('heading') %>">
</div>
<section id="bullets"></section>
<section id="paragraphs"></section>
<div>
  <ul>
    <li><a href='#' id='save-item'>Save Item</a></li>
    <li><a href='#' id='cancel-edit-item'>Cancel Edit Item</a></li>
  </ul>
</div>
